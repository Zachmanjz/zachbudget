
{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "noImplicitAny": false,
    "jsx": "react-jsx",
    "noEmit": true,
    "baseUrl": "."
  },
  "include": ["**/*.ts", "**/*.tsx", "global.d.ts"]
}
